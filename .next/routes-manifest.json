{
  "version": 3,
  "pages404": true,
  "appType": "app",
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "priority": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    },
    {
      "source": "/index.html",
      "destination": "/",
      "statusCode": 308,
      "regex": "^(?!/_next)/index\\.html(?:/)?$"
    },
    {
      "source": "/pages/auth/:path*.html",
      "destination": "/auth/:path*",
      "statusCode": 308,
      "regex": "^(?!/_next)/pages/auth(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))?\\.html(?:/)?$"
    },
    {
      "source": "/pages/k-col web software/protected/:path*.html",
      "destination": "/k-col/:path*",
      "statusCode": 308,
      "regex": "^(?!/_next)/pages/k-col web software/protected(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))?\\.html(?:/)?$"
    },
    {
      "source": "/pages/k-col web software/:path*.html",
      "destination": "/k-col/:path*",
      "statusCode": 308,
      "regex": "^(?!/_next)/pages/k-col web software(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))?\\.html(?:/)?$"
    },
    {
      "source": "/pages/K-product/:path*.html",
